{
  "name": "npminstall",
  "version": "0.1.0",
  "description": "Let npm install fast and easy",
  "main": "lib/index.js",
  "files": [
    "lib",
    "bin"
  ],
  "bin": {
    "npminstall": "bin/install.js"
  },
  "scripts": {
    "test": "mocha -r co-mocha -t 60000 test/*.test.js",
    "test-cov": "istanbul cover _mocha -- -r co-mocha -t 60000 test/*.test.js",
    "test-local": "npm_registry=https://registry.npm.taobao.org mocha -r co-mocha -t 60000 test/*.test.js",
    "lint": "eslint lib test",
    "ci": "npm run lint && npm run test-cov",
    "autod": "autod -w --prefix '~'"
  },
  "dependencies": {
    "await-event": "~1.0.0",
    "chalk": "~1.1.1",
    "co": "~4.6.0",
    "co-parallel": "~1.0.0",
    "debug": "~2.2.0",
    "mkdirp": "~0.5.1",
    "ms": "~0.7.1",
    "mz": "~2.3.0",
    "node-gyp": "~3.2.1",
    "npm-package-arg": "~4.1.0",
    "rimraf": "~2.5.1",
    "tar": "~2.2.1",
    "urllib": "~2.7.1",
    "utility": "~1.6.0"
  },
  "devDependencies": {
    "autod": "*",
    "co-mocha": "1",
    "eslint": "1",
    "eslint-config-egg": "~1.0.3",
    "istanbul": "*",
    "mocha": "*"
  },
  "homepage": "https://github.com/cnpm/npminstall",
  "repository": {
    "type": "git",
    "url": "git://github.com/cnpm/npminstall.git",
    "web": "https://github.com/cnpm/npminstall"
  },
  "bugs": {
    "url": "https://github.com/cnpm/npminstall/issues",
    "email": "m@fengmk2.com"
  },
  "keywords": [
    "npminstall"
  ],
  "engines": {
    "node": ">=4.2.3"
  },
